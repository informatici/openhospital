	<parameter name="todate" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT 	BLL_ID, 
	BLL_DATE, 
	BLL_UPDATE, 
	CONCAT(BLL_ID_PAT, " - ", BLL_PAT_NAME) AS PAT_NAME, 
	BLL_AMOUNT, 
    	IF(DATE(BLL_DATE) BETWEEN $P{fromdate} AND $P{todate}, BLL_AMOUNT, 0) BILLED,
	SUM(IF(DATE(BLP_DATE) BETWEEN $P{fromdate} AND $P{todate}, BLP_AMOUNT, 0)) INCOME,
	IF(DATE(BLL_DATE) BETWEEN $P{fromdate} AND $P{todate}, BLL_BALANCE, 0) RECEIVABLES,
	BLL_BALANCE, 
	BLL_STATUS
	FROM (
		SELECT * FROM BILLS LEFT JOIN BILLPAYMENTS ON BLL_ID = BLP_ID_BILL 
		WHERE BLL_STATUS <> 'D' AND BLL_STATUS <> 'X'
		AND (DATE(BLP_DATE) BETWEEN $P{fromdate} AND $P{todate}
			OR DATE(BLL_DATE) BETWEEN $P{fromdate} AND $P{todate})
	) AS BILLS_PLUS_PAYMENTS

GROUP BY BLL_ID
ORDER BY BLL_DATE]]></queryString>

