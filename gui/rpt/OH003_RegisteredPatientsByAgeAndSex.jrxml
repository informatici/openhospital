	<queryString><![CDATA[SELECT PAT_SEX AS SEX, "< 5" AS AGE, COUNT(PAT_ID) AS NUMBER
FROM PATIENT
WHERE DATE(PAT_TIMESTAMP) BETWEEN STR_TO_DATE($P{fromdate}, '%d/%m/%Y') AND STR_TO_DATE($P{todate}, '%d/%m/%Y')
AND PAT_SEX = 'F'
AND PAT_AGE < 5
GROUP BY PAT_SEX

UNION

SELECT PAT_SEX AS SEX, "5 - 10" AS AGE, COUNT(PAT_ID) AS NUMBER
FROM PATIENT
WHERE DATE(PAT_TIMESTAMP) BETWEEN STR_TO_DATE($P{fromdate}, '%d/%m/%Y') AND STR_TO_DATE($P{todate}, '%d/%m/%Y')
AND PAT_SEX = 'F'
AND PAT_AGE BETWEEN 5 AND 10
GROUP BY PAT_SEX

UNION

SELECT PAT_SEX AS SEX, "> 10" AS AGE, COUNT(PAT_ID) AS NUMBER
FROM PATIENT
WHERE DATE(PAT_TIMESTAMP) BETWEEN STR_TO_DATE($P{fromdate}, '%d/%m/%Y') AND STR_TO_DATE($P{todate}, '%d/%m/%Y')
AND PAT_SEX = 'F'
AND PAT_AGE > 10
GROUP BY PAT_SEX

UNION

SELECT PAT_SEX AS SEX, "< 5" AS AGE, COUNT(PAT_ID) AS NUMBER
FROM PATIENT
WHERE DATE(PAT_TIMESTAMP) BETWEEN STR_TO_DATE($P{fromdate}, '%d/%m/%Y') AND STR_TO_DATE($P{todate}, '%d/%m/%Y')
AND PAT_SEX = 'M'
AND PAT_AGE < 5
GROUP BY PAT_SEX

UNION

SELECT PAT_SEX AS SEX, "5 - 10" AS AGE, COUNT(PAT_ID) AS NUMBER
FROM PATIENT
WHERE DATE(PAT_TIMESTAMP) BETWEEN STR_TO_DATE($P{fromdate}, '%d/%m/%Y') AND STR_TO_DATE($P{todate}, '%d/%m/%Y')
AND PAT_SEX = 'M'
AND PAT_AGE BETWEEN 5 AND 10
GROUP BY PAT_SEX

UNION

SELECT PAT_SEX AS SEX, "> 10" AS AGE, COUNT(PAT_ID) AS NUMBER
FROM PATIENT
WHERE DATE(PAT_TIMESTAMP) BETWEEN STR_TO_DATE($P{fromdate}, '%d/%m/%Y') AND STR_TO_DATE($P{todate}, '%d/%m/%Y')
AND PAT_SEX = 'M'
AND PAT_AGE > 10
GROUP BY PAT_SEX]]></queryString>

