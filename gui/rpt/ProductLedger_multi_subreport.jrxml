	<queryString><![CDATA[SELECT * FROM (SELECT "1" AS ORDERING_FIELD,
	null AS MMV_ID, 
	DATE($P{fromdate}) AS MMV_DATE, 
    (SELECT MDSR_CODE FROM MEDICALDSR WHERE MDSR_ID = $P{productID}) AS MDSR_CODE, 
	(SELECT MDSR_DESC FROM MEDICALDSR WHERE MDSR_ID = $P{productID}) AS MDSR_DESC, 
	(SELECT MDSRT_DESC FROM MEDICALDSR JOIN MEDICALDSRTYPE ON MDSR_MDSRT_ID_A = MDSRT_ID_A WHERE MDSR_ID = $P{productID}) AS MDSRT_DESC, 
    "opening" AS MMVT_ID_A, 
    "opening" AS MMVT_TYPE, 
    "opening" AS MMVT_DESC, 
    "" AS MMV_REFNO, 
    "" AS MMV_LT_ID_A, 
    "" AS LT_DUE_DATE,
	null AS LT_COST, 
    "" AS MMV_FROM,
	"" AS SUP_NAME,
    "" AS WRD_NAME,
    (SELECT SUM(QTY_INPUT - QTY_OUTPUT) AS MMV_QTY
		FROM (SELECT MDSR_ID, MDSRT_DESC, MDSR_CODE, MDSR_DESC, MDSR_MIN_STOCK_QTI, LT_ID_A, LT_COST, SUM(INPUT) AS QTY_INPUT, SUM(OUTPUT) AS QTY_OUTPUT, LT_COST * SUM(INPUT) AS CHARGE, LT_COST * SUM(OUTPUT) AS DISCHARGE
			FROM (SELECT MDSR_ID, MDSRT_DESC, MDSR_CODE, MDSR_DESC, MDSR_MIN_STOCK_QTI, MMV_LT_ID_A, MMV_MMVT_ID_A, MMV_QTY, SUM(IF(MMVT_TYPE LIKE '%+%',MMV_QTY,0)) AS INPUT, SUM(IF(MMVT_TYPE LIKE '%-%', MMV_QTY, 0)) AS OUTPUT		
				FROM MEDICALDSR
				LEFT JOIN MEDICALDSRSTOCKMOV ON MDSR_ID = MMV_MDSR_ID
				LEFT JOIN MEDICALDSRTYPE ON MDSRT_ID_A = MDSR_MDSRT_ID_A
				LEFT JOIN MEDICALDSRSTOCKMOVTYPE ON MMV_MMVT_ID_A = MMVT_ID_A
				WHERE DATE(MMV_DATE) <= DATE($P{fromdate})
				AND MDSR_ID = $P{productID}
				GROUP BY MDSR_ID, MMV_LT_ID_A, MMV_MMVT_ID_A) AS QUANTITY
			JOIN MEDICALDSRLOT ON MMV_LT_ID_A = LT_ID_A
			GROUP BY LT_ID_A) AS VALUE
		GROUP BY MDSRT_DESC, MDSR_ID) AS MMV_QTY, 
	(SELECT SUM(CHARGE - DISCHARGE) AS VALUE
		FROM (SELECT MDSR_ID, MDSRT_DESC, MDSR_CODE, MDSR_DESC, MDSR_MIN_STOCK_QTI, LT_ID_A, LT_COST, SUM(INPUT) AS QTY_INPUT, SUM(OUTPUT) AS QTY_OUTPUT, LT_COST * SUM(INPUT) AS CHARGE, LT_COST * SUM(OUTPUT) AS DISCHARGE
			FROM (SELECT MDSR_ID, MDSRT_DESC, MDSR_CODE, MDSR_DESC, MDSR_MIN_STOCK_QTI, MMV_LT_ID_A, MMV_MMVT_ID_A, MMV_QTY, SUM(IF(MMVT_TYPE LIKE '%+%',MMV_QTY,0)) AS INPUT, SUM(IF(MMVT_TYPE LIKE '%-%', MMV_QTY, 0)) AS OUTPUT		
				FROM MEDICALDSR
				LEFT JOIN MEDICALDSRSTOCKMOV ON MDSR_ID = MMV_MDSR_ID
				LEFT JOIN MEDICALDSRTYPE ON MDSRT_ID_A = MDSR_MDSRT_ID_A
				LEFT JOIN MEDICALDSRSTOCKMOVTYPE ON MMV_MMVT_ID_A = MMVT_ID_A
				WHERE DATE(MMV_DATE) <= DATE($P{fromdate})
				AND MDSR_ID = $P{productID}
				GROUP BY MDSR_ID, MMV_LT_ID_A, MMV_MMVT_ID_A) AS QUANTITY
			JOIN MEDICALDSRLOT ON MMV_LT_ID_A = LT_ID_A
			GROUP BY LT_ID_A) AS VALUE
		GROUP BY MDSRT_DESC, MDSR_ID) AS VALUE,
	(SELECT SUM(CHARGE - DISCHARGE) AS START_VALUE
		FROM (SELECT MDSR_ID, MDSRT_DESC, MDSR_CODE, MDSR_DESC, MDSR_MIN_STOCK_QTI, LT_ID_A, LT_COST, SUM(INPUT) AS QTY_INPUT, SUM(OUTPUT) AS QTY_OUTPUT, LT_COST * SUM(INPUT) AS CHARGE, LT_COST * SUM(OUTPUT) AS DISCHARGE
			FROM (SELECT MDSR_ID, MDSRT_DESC, MDSR_CODE, MDSR_DESC, MDSR_MIN_STOCK_QTI, MMV_LT_ID_A, MMV_MMVT_ID_A, MMV_QTY, SUM(IF(MMVT_TYPE LIKE '%+%',MMV_QTY,0)) AS INPUT, SUM(IF(MMVT_TYPE LIKE '%-%', MMV_QTY, 0)) AS OUTPUT		
				FROM MEDICALDSR
				LEFT JOIN MEDICALDSRSTOCKMOV ON MDSR_ID = MMV_MDSR_ID
				LEFT JOIN MEDICALDSRTYPE ON MDSRT_ID_A = MDSR_MDSRT_ID_A
				LEFT JOIN MEDICALDSRSTOCKMOVTYPE ON MMV_MMVT_ID_A = MMVT_ID_A
				WHERE DATE(MMV_DATE) <= DATE($P{fromdate})
				AND MDSR_ID = $P{productID}
				GROUP BY MDSR_ID, MMV_LT_ID_A, MMV_MMVT_ID_A) AS QUANTITY
			JOIN MEDICALDSRLOT ON MMV_LT_ID_A = LT_ID_A
			GROUP BY LT_ID_A) AS VALUE
		GROUP BY MDSRT_DESC, MDSR_ID) AS START_VALUE

UNION

SELECT 
    "2" AS ORDERING_FIELD, MMV_ID, MMV_DATE, MDSR_CODE, MDSR_DESC, MDSRT_DESC, MMVT_ID_A, MMVT_TYPE, MMVT_DESC, MMV_REFNO, MMV_LT_ID_A, LT_DUE_DATE, LT_COST, MMV_FROM, SUP_NAME, WRD_NAME, MMV_QTY, IF(MMVT_TYPE LIKE "%+%", (LT_COST * MMV_QTY), -(LT_COST * MMV_QTY)) AS VALUE, null AS START_VALUE
FROM
    ((MEDICALDSRSTOCKMOVTYPE
    JOIN MEDICALDSRSTOCKMOV ON MMVT_ID_A = MMV_MMVT_ID_A)
    JOIN (MEDICALDSR
    JOIN MEDICALDSRTYPE ON MDSR_MDSRT_ID_A = MDSRT_ID_A) ON MMV_MDSR_ID = MDSR_ID)
        LEFT JOIN
    WARD ON MMV_WRD_ID_A = WRD_ID_A
        LEFT JOIN
    MEDICALDSRLOT ON MMV_LT_ID_A = LT_ID_A
		LEFT JOIN
	SUPPLIER ON MMV_FROM = SUP_ID
WHERE
    (MDSR_ID = $P{productID})
        AND (DATE(MMV_DATE) BETWEEN DATE($P{fromdate}) AND DATE($P{todate}))
-- ORDER BY MMV_DATE ASC, MMV_ID ASC

UNION

SELECT "3" AS ORDERING_FIELD,
	null AS MMV_ID, 
	DATE($P{todate}) AS MMV_DATE, 
    (SELECT MDSR_CODE FROM MEDICALDSR WHERE MDSR_ID = $P{productID}) AS MDSR_CODE, 
	(SELECT MDSR_DESC FROM MEDICALDSR WHERE MDSR_ID = $P{productID}) AS MDSR_DESC, 
	(SELECT MDSRT_DESC FROM MEDICALDSR JOIN MEDICALDSRTYPE ON MDSR_MDSRT_ID_A = MDSRT_ID_A WHERE MDSR_ID = $P{productID}) AS MDSRT_DESC, 
    "closing" AS MMVT_ID_A, 
    "closing" AS MMVT_TYPE, 
    "Stock Quantity at the end of period" AS MMVT_DESC, 
    "" AS MMV_REFNO, 
    "" AS MMV_LT_ID_A, 
    "" AS LT_DUE_DATE, 
	null AS LT_COST, 
    "" AS MMV_FROM,
	"" AS SUP_NAME,
    "" AS WRD_NAME,
    (SELECT SUM(QTY_INPUT - QTY_OUTPUT) AS MMV_QTY
		FROM (SELECT MDSR_ID, MDSRT_DESC, MDSR_CODE, MDSR_DESC, MDSR_MIN_STOCK_QTI, LT_ID_A, LT_COST, SUM(INPUT) AS QTY_INPUT, SUM(OUTPUT) AS QTY_OUTPUT, LT_COST * SUM(INPUT) AS CHARGE, LT_COST * SUM(OUTPUT) AS DISCHARGE
			FROM (SELECT MDSR_ID, MDSRT_DESC, MDSR_CODE, MDSR_DESC, MDSR_MIN_STOCK_QTI, MMV_LT_ID_A, MMV_MMVT_ID_A, MMV_QTY, SUM(IF(MMVT_TYPE LIKE '%+%',MMV_QTY,0)) AS INPUT, SUM(IF(MMVT_TYPE LIKE '%-%', MMV_QTY, 0)) AS OUTPUT		
				FROM MEDICALDSR
				LEFT JOIN MEDICALDSRSTOCKMOV ON MDSR_ID = MMV_MDSR_ID
				LEFT JOIN MEDICALDSRTYPE ON MDSRT_ID_A = MDSR_MDSRT_ID_A
				LEFT JOIN MEDICALDSRSTOCKMOVTYPE ON MMV_MMVT_ID_A = MMVT_ID_A
				WHERE DATE(MMV_DATE) <= DATE($P{todate})
				AND MDSR_ID = $P{productID}
				GROUP BY MDSR_ID, MMV_LT_ID_A, MMV_MMVT_ID_A) AS QUANTITY
			JOIN MEDICALDSRLOT ON MMV_LT_ID_A = LT_ID_A
			GROUP BY LT_ID_A) AS VALUE
		GROUP BY MDSRT_DESC, MDSR_ID) AS MMV_QTY, 
	(SELECT SUM(CHARGE - DISCHARGE) AS VALUE
		FROM (SELECT MDSR_ID, MDSRT_DESC, MDSR_CODE, MDSR_DESC, MDSR_MIN_STOCK_QTI, LT_ID_A, LT_COST, SUM(INPUT) AS QTY_INPUT, SUM(OUTPUT) AS QTY_OUTPUT, LT_COST * SUM(INPUT) AS CHARGE, LT_COST * SUM(OUTPUT) AS DISCHARGE
			FROM (SELECT MDSR_ID, MDSRT_DESC, MDSR_CODE, MDSR_DESC, MDSR_MIN_STOCK_QTI, MMV_LT_ID_A, MMV_MMVT_ID_A, MMV_QTY, SUM(IF(MMVT_TYPE LIKE '%+%',MMV_QTY,0)) AS INPUT, SUM(IF(MMVT_TYPE LIKE '%-%', MMV_QTY, 0)) AS OUTPUT		
				FROM MEDICALDSR
				LEFT JOIN MEDICALDSRSTOCKMOV ON MDSR_ID = MMV_MDSR_ID
				LEFT JOIN MEDICALDSRTYPE ON MDSRT_ID_A = MDSR_MDSRT_ID_A
				LEFT JOIN MEDICALDSRSTOCKMOVTYPE ON MMV_MMVT_ID_A = MMVT_ID_A
				WHERE DATE(MMV_DATE) <= DATE($P{todate})
				AND MDSR_ID = $P{productID}
				GROUP BY MDSR_ID, MMV_LT_ID_A, MMV_MMVT_ID_A) AS QUANTITY
			JOIN MEDICALDSRLOT ON MMV_LT_ID_A = LT_ID_A
			GROUP BY LT_ID_A) AS VALUE
		GROUP BY MDSRT_DESC, MDSR_ID) AS VALUE,
	(SELECT SUM(CHARGE - DISCHARGE) AS START_VALUE
		FROM (SELECT MDSR_ID, MDSRT_DESC, MDSR_CODE, MDSR_DESC, MDSR_MIN_STOCK_QTI, LT_ID_A, LT_COST, SUM(INPUT) AS QTY_INPUT, SUM(OUTPUT) AS QTY_OUTPUT, LT_COST * SUM(INPUT) AS CHARGE, LT_COST * SUM(OUTPUT) AS DISCHARGE
			FROM (SELECT MDSR_ID, MDSRT_DESC, MDSR_CODE, MDSR_DESC, MDSR_MIN_STOCK_QTI, MMV_LT_ID_A, MMV_MMVT_ID_A, MMV_QTY, SUM(IF(MMVT_TYPE LIKE '%+%',MMV_QTY,0)) AS INPUT, SUM(IF(MMVT_TYPE LIKE '%-%', MMV_QTY, 0)) AS OUTPUT		
				FROM MEDICALDSR
				LEFT JOIN MEDICALDSRSTOCKMOV ON MDSR_ID = MMV_MDSR_ID
				LEFT JOIN MEDICALDSRTYPE ON MDSRT_ID_A = MDSR_MDSRT_ID_A
				LEFT JOIN MEDICALDSRSTOCKMOVTYPE ON MMV_MMVT_ID_A = MMVT_ID_A
				WHERE DATE(MMV_DATE) <= DATE($P{todate})
				AND MDSR_ID = $P{productID}
				GROUP BY MDSR_ID, MMV_LT_ID_A, MMV_MMVT_ID_A) AS QUANTITY
			JOIN MEDICALDSRLOT ON MMV_LT_ID_A = LT_ID_A
			GROUP BY LT_ID_A) AS VALUE
		GROUP BY MDSRT_DESC, MDSR_ID) AS START_VALUE

) AS BEGIN_MOV_END
ORDER BY ORDERING_FIELD ASC, MMV_DATE ASC, MMV_REFNO DESC]]></queryString>

