	<queryString><![CDATA[SELECT BLL_DATE, BLL_UPDATE, BLI_ITEM_DESC, SUM(BLI_QTY) AS QTY, SUM(BLI_ITEM_AMOUNT) * BLI_QTY AS TOTAL
FROM BILLS, BILLITEMS
WHERE BLL_STATUS = 'C'
AND DATE(BLL_DATE) between STR_TO_DATE($P{fromdate},'%d/%m/%Y') and STR_TO_DATE($P{todate},'%d/%m/%Y')
AND BLL_ID = BLI_ID_BILL
AND BLI_IS_PRICE = 1
GROUP BY BLI_ID_PRICE
ORDER BY TOTAL DESC]]></queryString>

