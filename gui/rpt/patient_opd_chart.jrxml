SELECT "Second Diagnosis:" AS FIELD, CONCAT((SELECT DIS_DESC FROM DISEASE WHERE DIS_ID_A = OPD_DIS_ID_A_2)) AS DETAIL FROM OPD WHERE OPD_ID = $P{opdID}
UNION
SELECT "Third Diagnosis:" AS FIELD, CONCAT((SELECT DIS_DESC FROM DISEASE WHERE DIS_ID_A = OPD_DIS_ID_A_3)) AS DETAIL FROM OPD WHERE OPD_ID = $P{opdID}]]></queryString>

