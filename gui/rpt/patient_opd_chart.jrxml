UNION
SELECT "Type:" AS FIELD, IF(OPD_NEW_PAT='N','New Attendance','Re-Attendance') AS DETAIL FROM OPD WHERE OPD_ID = $P{opdID}
UNION
SELECT "Clinical Remarks:" AS FIELD, OPD_NOTE AS DETAIL FROM OPD WHERE OPD_ID = $P{opdID}
UNION
SELECT "Diagnosis:" AS FIELD, CONCAT((SELECT DIS_DESC FROM DISEASE WHERE DIS_ID_A = OPD_DIS_ID_A)) AS DETAIL FROM OPD WHERE OPD_ID = $P{opdID}
